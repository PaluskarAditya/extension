{
    "name": "Injector",
    "description": "This is a chrome Injector extension",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["app.js", "login.js"]
        }
    ],
    "host_permissions": ["<all_urls>"],
    "action": {
        "default_popup": "login.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "scripting",
        "tabs",
        "activeTab",
        "storage"
    ]
}